                                sDir := strDir,
                                sFileName := strFileName,
                                sExt := strExt );
								
								getStrInstanceName := MID (strExt,TC2_STANDARD.LEN(strExt),2);
]]></ST>
    </Implementation>
    <ObjectProperties />
