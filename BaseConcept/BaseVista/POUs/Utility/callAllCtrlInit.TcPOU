FOR uinIdx := 0 TO uinCtrlInstLength DO
	IF pCtrlInst[uinIdx] <> 0 THEN
		pCtrlInst[uinIdx]^.CtrlInit(); // init ctrl and add childs
	END_IF
END_FOR
]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>