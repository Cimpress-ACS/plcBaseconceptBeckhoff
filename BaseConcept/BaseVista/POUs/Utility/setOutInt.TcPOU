limitations under the License. *)

// Copy real values
p_Output^.state := p_Output^.intern;

IF (p_Output^.EnForce AND bol_GlobalEnableForce) THEN
	p_Output^.map := p_Output^.force;
ELSE
	p_Output^.map := p_Output^.intern;
END_IF;

setOutInt := p_Output^.map;]]></ST>
    </Implementation>
  </POU>
