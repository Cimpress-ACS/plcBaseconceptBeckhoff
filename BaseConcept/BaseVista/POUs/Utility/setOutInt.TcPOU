<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4019.2">
  <POU Name="setOutInt" Id="{e9ef2697-df64-4566-be18-d30e900ebf53}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION setOutInt : INT
VAR_INPUT
	bol_GlobalEnableForce: BOOL;
	p_Output: POINTER TO T_Ctrl_IoInt;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[(* Copyright 2017 Cimpress

